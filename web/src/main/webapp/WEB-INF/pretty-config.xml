		<view-id>/secured/admin/people.faces</view-id>
		<action onPostback="false">#{personBean.resetList}</action>
	</url-mapping>
    
    <url-mapping id="adminPeople">
        <pattern>/app/admin/people</pattern>
        <view-id>/secured/admin/people.faces</view-id>
