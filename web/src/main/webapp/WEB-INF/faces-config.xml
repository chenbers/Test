			com.inthinc.pro.backing.listener.MultiPageMessagesSupport
		</phase-listener>
	</lifecycle>
	
    <!-- 
	<lifecycle>
		<phase-listener>com.inthinc.pro.util.DebugPhaseTracker</phase-listener>
	</lifecycle>
    -->
	
 
    <!-- Facelets support -->
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>en_US</default-locale>
			<supported-locale>en_US</supported-locale>
            <supported-locale>ro</supported-locale>
            <supported-locale>es</supported-locale>
            <supported-locale>ja</supported-locale>
            <supported-locale>fr</supported-locale>
            <supported-locale>de</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.inthinc.pro.resources.Messages</base-name>
			<var>messages</var>
		</resource-bundle>
		<message-bundle>com.inthinc.pro.resources.Messages</message-bundle>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    
</faces-config>
