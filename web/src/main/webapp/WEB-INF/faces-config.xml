<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2"
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	<!-- converters -->
	<converter>
		<converter-id>ScoreConverter</converter-id>
		<converter-class>com.inthinc.pro.convert.ScoreConverter</converter-class>
	</converter>
	<converter>
		<converter-id>TimeZoneConverter</converter-id>
		<converter-class>com.inthinc.pro.convert.TimeZoneConverter</converter-class>
	</converter>
	<converter>
		<converter-id>SimpleBeanConverter</converter-id>
		<converter-class>com.inthinc.pro.convert.SimpleBeanConverter</converter-class>
	</converter>
	<converter>
		<converter-id>StateConverter</converter-id>
		<converter-class>com.inthinc.pro.convert.StateConverter</converter-class>
	</converter>
	<converter>
		<converter-id>RoleConverter</converter-id>
		<converter-class>com.inthinc.pro.convert.RoleConverter</converter-class>
	</converter>

	<!-- managed beans -->
	
	<!-- navigation -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_test</from-outcome>
			<to-view-id>/pages/testPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_todo</from-outcome>
			<to-view-id>/pages/todo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_home</from-outcome>
			<to-view-id>/secured/home.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_retrieveCredentialsComplete</from-outcome>
			<to-view-id>/retrieveCredentialsComplete.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_reports</from-outcome>
			<to-view-id>/secured/reports/drivers.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_redFlags</from-outcome>
			<to-view-id>/secured/redFlags.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_liveFleet</from-outcome>
			<to-view-id>/secured/liveFleet.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_admin</from-outcome>
			<to-view-id>/secured/admin/people.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_adminPeople</from-outcome>
			<to-view-id>/secured/admin/people.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminPerson</from-outcome>
        <to-view-id>/secured/admin/person.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminAddPerson</from-outcome>
        <to-view-id>/secured/admin/editPerson.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminEditPerson</from-outcome>
        <to-view-id>/secured/admin/editPerson.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_adminVehicles</from-outcome>
			<to-view-id>/secured/admin/vehicles.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminVehicle</from-outcome>
        <to-view-id>/secured/admin/vehicle.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminAddVehicle</from-outcome>
        <to-view-id>/secured/admin/editVehicle.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminEditVehicle</from-outcome>
        <to-view-id>/secured/admin/editVehicle.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_adminDevices</from-outcome>
			<to-view-id>/secured/admin/devices.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminDevice</from-outcome>
        <to-view-id>/secured/admin/device.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminAddDevice</from-outcome>
        <to-view-id>/secured/admin/editDevice.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminEditDevice</from-outcome>
        <to-view-id>/secured/admin/editDevice.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminZones</from-outcome>
        <to-view-id>/secured/admin/zones.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminZoneAlerts</from-outcome>
        <to-view-id>/secured/admin/zoneAlerts.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminZoneAlert</from-outcome>
        <to-view-id>/secured/admin/zoneAlert.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminAddZoneAlert</from-outcome>
        <to-view-id>/secured/admin/editZoneAlert.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminEditZoneAlert</from-outcome>
        <to-view-id>/secured/admin/editZoneAlert.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminOrganization</from-outcome>
        <to-view-id>/secured/admin/organization.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminSBSCR</from-outcome>
        <to-view-id>/secured/admin/speedByStreetChangeRequest.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminEditGroup</from-outcome>
        <to-view-id>/secured/admin/editGroup.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminRedFlags</from-outcome>
        <to-view-id>/secured/admin/redFlags.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminRedFlag</from-outcome>
        <to-view-id>/secured/admin/redFlag.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminAddRedFlag</from-outcome>
        <to-view-id>/secured/admin/editRedFlag.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>go_adminEditRedFlag</from-outcome>
        <to-view-id>/secured/admin/editRedFlag.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_division</from-outcome>
			<to-view-id>/secured/division.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_fleetDetail</from-outcome>
			<to-view-id>/secured/fleetDetail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_divisionDetail</from-outcome>
			<to-view-id>/secured/divisionDetail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_team</from-outcome>
			<to-view-id>/secured/team.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_driver</from-outcome>
			<to-view-id>/secured/driver.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_vehicle</from-outcome>
			<to-view-id>/secured/vehicle.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_reportDriverSpeed</from-outcome>
			<to-view-id>/secured/reports/driverSpeed.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_reportDriverStyle</from-outcome>
			<to-view-id>/secured/reports/driverStyle.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
		<navigation-case>
			<from-outcome>go_reportDriverSeatBelt</from-outcome>
			<to-view-id>/secured/reports/driverSeatBelt.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_driverTrips</from-outcome>
			<to-view-id>/secured/driverTrips.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- validators -->
	
	<!-- components -->
	<component>
   		<component-type>com.inthinc.pro.security.jsftaglib.Authorize</component-type>
		<component-class>com.inthinc.pro.security.jsftaglib.Authorize</component-class>
	</component> 
	<component>
		<component-type>com.inthinc.pro.security.jsftaglib.Authentication</component-type>
		<component-class>com.inthinc.pro.security.jsftaglib.Authentication</component-class>
	</component>
 
	<!-- render kits -->
	
	<!-- lifecycle -->
	<lifecycle>
		<phase-listener>com.inthinc.pro.util.DebugPhaseTracker</phase-listener>
	</lifecycle>
	
 
    <!-- Facelets support -->
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.inthinc.pro.resources.Messages</base-name>
			<var>messages</var>
		</resource-bundle>
		<message-bundle>com.inthinc.pro.resources.Messages</message-bundle>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    
</faces-config>
