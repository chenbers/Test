<pretty-config
	xmlns="http://ocpsoft.com/prettyfaces-xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces-xsd 
	http://ocpsoft.com/xml/ns/prettyfaces/pretty-1.0.xsd">
	<url-mapping id="login">
		<pattern>/login</pattern>
		<view-id>/login.faces</view-id>
	</url-mapping>

	<url-mapping id="kiosk">
		<pattern>/app/kiosk</pattern>
		<view-id>/secured/hosDriver.faces</view-id>
	</url-mapping>
    <url-mapping id="current">
        <pattern>/app/current</pattern>
        <view-id>/secured/hosCurrentStatus.faces</view-id>
    </url-mapping>    
    <url-mapping id="print">
        <pattern>/app/print</pattern>
        <view-id>/secured/hosPrintLogs.faces</view-id>
    </url-mapping>    
    <url-mapping id="export">
        <pattern>/app/export</pattern>
        <view-id>/secured/hosExportLogs.faces</view-id>
    </url-mapping>    

</pretty-config>