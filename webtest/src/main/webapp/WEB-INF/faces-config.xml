<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">

	<!-- converters -->
	
	<!-- Components -->

	<!-- managed beans -->
	
	<!-- navigation -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_table</from-outcome>
			<to-view-id>/pages/testTableRedFlag.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- validators -->

	<!-- components -->
	<!-- render kits -->
	
	<!-- lifecycle -->
	
    <!-- 
	<lifecycle>
		<phase-listener>com.inthinc.pro.util.DebugPhaseTracker</phase-listener>
	</lifecycle>
    -->
	
 
    <!-- Facelets support -->
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<locale-config>
			<default-locale>en_US</default-locale>
			<supported-locale>en_US</supported-locale>
            <supported-locale>ro</supported-locale>
            <supported-locale>es</supported-locale>
            <supported-locale>ja</supported-locale>
            <supported-locale>fr</supported-locale>
            <supported-locale>de</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.inthinc.pro.resources.Messages</base-name>
			<var>messages</var>
		</resource-bundle>
		<message-bundle>com.inthinc.pro.resources.Messages</message-bundle>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    
</faces-config>
